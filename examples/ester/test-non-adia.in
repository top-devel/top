#!@PYTHON@

import top

if __name__ == '__main__':
    l = top.load('eq_non_adiabatic')
    l.read_dati('@builddir@/dati-non-adia')
    l.init_model('@builddir@/rasalhague.h5')
    res = l.run_arncheb(l.dati.shift_real + 1J*l.dati.shift_imag)

    for idom in range(0, res.ndom-1):
        res.plot(idom, 2, 'Phi%d' % (idom+1))
