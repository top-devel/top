#!@PYTHON@

import top

if __name__ == '__main__':
    top.load('eq_ESTER_old_all_lagrange')
    top.read_dati('@builddir@/dati')
    # top.init_model('@builddir@/star.h5')
    top.init_model(top.dati.filename)
    top.run_arncheb(top.dati.shift)

    # retrieve the results
    res = top.results()
    for idom in range(0, res.ndom-1):
        top.plot(idom, 2, 'Phi%d' % (idom+1))
