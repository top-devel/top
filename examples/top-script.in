#!@PYTHON@

import os
import sys
sys.path.append('@prefix@/bin')
import top

print('eq_poly_ester:')
top.load('eq_poly_ester')
top.read_dati('@datadir@/top/models/poly_ester/dati')
model = top.get_from_dati('dirmodel')
shift = float(top.get_from_dati('shift'))
top.init_model(model)
print('dirmodel: ' + model)
print('shift: %e' % shift)
top.run_arncheb(shift)

vecps = top.get_vecps()
valps = top.get_valps()
for v in valps:
    print("%e" % v)
print('grid: ' + str(top.get_grid()))


